<?xml version="1.0" encoding="UTF-8"?>
<robot xmlns="http://www.ros.org/schema/srdf" name="ur3">
  <!-- MoveIt SRDF for UR3 -->
  <group name="ur_manipulator">
    <chain base_link="base_link" tip_link="tool0" />
  </group>
  <group_state name="home" group="ur_manipulator">
    <joint name="elbow_joint" value="0" />
    <joint name="shoulder_lift_joint" value="-1.5707" />
    <joint name="shoulder_pan_joint" value="0" />
    <joint name="wrist_1_joint" value="0" />
    <joint name="wrist_2_joint" value="0" />
    <joint name="wrist_3_joint" value="0" />
  </group_state>
  <virtual_joint name="world_joint" type="fixed" parent_frame="world" child_link="base_link"/>
  <disable_collisions link1="base_link" link2="base_link_inertia" reason="Adjacent" />
  <disable_collisions link1="base_link_inertia" link2="shoulder_link" reason="Adjacent" />
  <disable_collisions link1="tool0" link2="wrist_1_link" reason="Never" />
  <disable_collisions link1="tool0" link2="wrist_2_link" reason="Never" />
  <disable_collisions link1="tool0" link2="wrist_3_link" reason="Adjacent" />
  <disable_collisions link1="forearm_link" link2="upper_arm_link" reason="Adjacent" />
  <disable_collisions link1="forearm_link" link2="wrist_1_link" reason="Adjacent" />
  <disable_collisions link1="shoulder_link" link2="upper_arm_link" reason="Adjacent" />
  <disable_collisions link1="wrist_1_link" link2="wrist_2_link" reason="Adjacent" />
  <disable_collisions link1="wrist_1_link" link2="wrist_3_link" reason="Never" />
  <disable_collisions link1="wrist_2_link" link2="wrist_3_link" reason="Adjacent" />
  <disable_collisions link1="base_link" link2="station" reason="Never" />
  <disable_collisions link1="base_link" link2="station_cube" reason="Never" />
  <disable_collisions link1="base_link" link2="station_top" reason="Never" />
  <disable_collisions link1="base_link" link2="robot_mount_cylinder" reason="Never" />
  <disable_collisions link1="tool0" link2="station" reason="Never" />
  <disable_collisions link1="tool0" link2="station_cube" reason="Never" />
  <disable_collisions link1="tool0" link2="station_top" reason="Never" />
  <disable_collisions link1="tool0" link2="robot_mount_cylinder" reason="Never" />

  <!-- Kollisionen base_link_inertia mit station_cube und uX_cube deaktiviert -->
  <disable_collisions link1="base_link_inertia" link2="station_cube" reason="Never" />
  <disable_collisions link1="base_link_inertia" link2="u1_cube" reason="Never" />
  <disable_collisions link1="base_link_inertia" link2="u2_cube" reason="Never" />
  <disable_collisions link1="base_link_inertia" link2="u3_cube" reason="Never" />
  <disable_collisions link1="base_link_inertia" link2="u4_cube" reason="Never" />

  <!-- Kollisionen zwischen Base und Tisch bleiben aktiv (aus ur_macro.srdf.xacro Ã¼bernommen) -->
  <disable_collisions link1="station_cube" link2="u1_cube" reason="Table" />
  <disable_collisions link1="station_cube" link2="u2_cube" reason="Table" />
  <disable_collisions link1="station_cube" link2="u3_cube" reason="Table" />
  <disable_collisions link1="station_cube" link2="u4_cube" reason="Table" />
  <disable_collisions link1="u1_cube" link2="u2_cube" reason="Table" />
  <disable_collisions link1="u1_cube" link2="u3_cube" reason="Table" />
  <disable_collisions link1="u2_cube" link2="u4_cube" reason="Table" />
  <disable_collisions link1="u3_cube" link2="u4_cube" reason="Table" />
</robot>
